# Backend Production Configuration
# Copy this to .env on your production server and fill in your values

PORT=5000

# PostgreSQL Database Credentials
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name

# JWT Secret (generate a secure random string)
JWT_SECRET=your_secure_random_jwt_secret_here

# CORS - Frontend domain(s) allowed to access the API
CORS_ORIGIN=https://vclubshop.co.in

# Base URL for the API (optional, for reference)
BASE_URL=https://api.vclubshop.co.in